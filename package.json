{
	"name": "custom-bits-monorepo",
	"version": "0.0.0",
	"license": "MIT",
	"type": "module",
	"workspaces": [
		"configs/*",
		"libs/*",
		"demo"
	],
	"scripts": {
		"prettier:configs": "prettier ./configs --write --config ./configs/prettier/index.json",
		"prettier:check": "npm run prettier:check --workspaces --if-present",
		"prettier": "npm run prettier --workspaces --if-present",
		"lint:check": "npm run lint:check --workspaces --if-present",
		"dev": "cd demo && vite"
	},
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@custom-bits/prettier": "*"
	}
}
