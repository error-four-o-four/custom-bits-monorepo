{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Vite App",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "ES2020",
		"useDefineForClassFields": true,
		"module": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],

		"moduleResolution": "Bundler",
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"exclude": ["node_modules"]
}
